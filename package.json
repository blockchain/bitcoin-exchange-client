{
  "name": "bitcoin-exchange-client",
  "version": "0.6.0",
  "description": "Bitcoin Exchange base classes, use with e.g. coinify-exchange-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchain/bitcoin-exchange-client.git"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "prepublish": "node_modules/.bin/babel src --presets babel-preset-es2015 --out-dir lib",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "keywords": [
    "Blockchain"
  ],
  "author": "Sjors Provoost <sjors@blockchain.com>",
  "license": "ISC",
  "dependencies": {
    "isomorphic-fetch": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-polyfill": "6.16.*",
    "babel-preset-es2015": "6.16.*",
    "babelify": "7.3.*",
    "browserify": "13.*",
    "browserify-istanbul": "2.0.*",
    "coveralls": "^2.11.14",
    "eslint": "2.*",
    "fetch-mock": "^5.1.2",
    "git-changelog": "1.0.*",
    "jasmine-core": "^2.2.0",
    "jasmine-es6-promise-matchers": "^2.0.1",
    "karma": "0.13.*",
    "karma-browserify": "5.1.*",
    "karma-coverage": "1.1.*",
    "karma-jasmine": "1.0.*",
    "karma-osx-reporter": "0.2.*",
    "karma-phantomjs-launcher": "1.0.*",
    "phantomjs-prebuilt": "2.1.*",
    "proxyquireify": "3.2.*",
    "semistandard": "8.*",
    "watchify": "3.*"
  },
  "semistandard": {
    "ignore": [
      "tests/"
    ],
    "globals": [
      "fetch",
      "beforeEach",
      "afterEach",
      "describe",
      "expect",
      "it",
      "JasminePromiseMatchers",
      "jasmine",
      "spyOn",
      "pending",
      "fail"
    ]
  },
  "engines": {
    "node": ">=7.0"
  }
}
